<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="assets_frontend" inherit_id="website.assets_frontend" name="SIT Scss Assets">
            <xpath expr="." position="inside">
                <link rel="stylesheet" type="text/scss" href="/sit_portal_dashboard/static/src/scss/style.scss"/>
                <script type="text/javascript" src="/sit_portal_dashboard/static/src/js/custom.js"></script>
            </xpath>
        </template>
        <template id="_assets_helpers" inherit_id="web._assets_helpers" name="Assets Helper">
            <xpath expr="." position="inside">
                <link rel="stylesheet" type="text/scss" href="/sit_portal_dashboard/static/src/scss/_variables.scss"/>
            </xpath>
        </template>
    </data>
</odoo>
